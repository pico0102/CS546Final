<body>
    <header>
        <h1>Log In</h1>
    </header>

    <form id="static-form" action="/login" method="post">
        <div>
            <label>Username:</label>
            <input type="text" name="username" id="user"/>
        </div>
        <div>
            <label>Password:</label>
            <input type="password" name="password" id="pass"/>
        </div>
        <div>
            <input type="submit" value="Log In"/>
        </div>
    </form>
    
</body>