<article class="dashboard">
  <h1>My Dashboard</h1>

  <p>HEY:</p>
  <p>{{user.profile.name}}</p>

  <h2>Games you might like from {{game.name}}</h2>
  {{#each games}}  
    <article class="game">
      <h2>{{this.name}}</h2>
      <img src={{this.art}} alt="Game Art">
      <p>{{this.keywords}}</p>
      <p>{{this.description}}</p>
    </article>
  {{/each}}

  <h2>Popular Games</h2>
  {{#each popgames}}  
    <article class="game">
      <h2>{{this.name}}</h2>
      <img src={{this.art}} alt="Game Art">
      <p>{{this.keywords}}</p>
      <p>{{this.description}}</p>
    </article>
  {{/each}}

</article>